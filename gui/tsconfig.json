{
  "compilerOptions": {
    "target": "esNext",
    "module": "ES2020",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,

    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["vite/client"],
    "isolatedModules": true
  }
}
