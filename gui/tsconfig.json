{
  "compilerOptions": {
    "target": "esNext",
    "jsx": "preserve",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,

    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["vite/client"],
    "isolatedModules": true
  }
}
